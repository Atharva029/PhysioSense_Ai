{
  "name": "physiosense-ai",
  "version": "1.0.0",
  "description": "AI-powered physiotherapy exercise monitoring system",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:5001\" \"npm run client:3001\"",
    "server": "cd server && npm run dev",
    "server:5001": "cd server && set PORT=5001 && npm run dev",
    "client": "cd client && npm start",
    "client:3001": "cd client && set PORT=3001 && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "physiotherapy",
    "ai",
    "computer-vision",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
